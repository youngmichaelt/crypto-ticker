{
  "particles":{
    "number":{
      "value": 100
    },
    "color":{
      "value":"#ecf0f1"
    },
    "shape": {
      "type": "image",
      "image":{
        "src": "https://bitstickers.net/wp-content/uploads/2014/02/btc-mono-ring-orange.jpg",
        "src": "https://cryptoindex.guru/wp-content/uploads/2018/01/bitcoin_PNG48.png"
      },

      "stroke":{
        "width":1,
        "color":"#ccc"
      }

    },
    "size":{
      "value":50,
      "random":true,
      "anim": {
        "enable": true,
        "speed": 5
      }
    },
    "opacity":{
      "value": 1
    }},
    "interactivity": {
      "detect_on": "window",
      "events": {
        "onhover": {
          "enable": true,
          "mode": "repulse"
        },
        "onclick": {
          "enable": true,
          "mode": "push"
        },
        "resize": true
      },
      "modes": {
        "grab": {
          "distance": 400,
          "line_linked": {
            "opacity": 1
          }
        },
        "bubble": {
          "distance": 400,
          "size": 40,
          "duration": 2,
          "opacity": 8,
          "speed": 3
        },
        "repulse": {
          "distance": 75
        },
        "push": {
          "particles_nb": 4
        },
        "remove": {
          "particles_nb": 2
        }
      }
    }




  }
